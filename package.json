  },
  "dependencies": {
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "~8.56.0",
    "eslint-plugin-jest": "^27.9.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.2.2"
}
